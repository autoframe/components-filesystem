{
    "type": "library",
    "name": "autoframe/components-filesystem",
    "description": "Core File System Utils Autoframe Framework",
    "license": "MIT",
    "homepage": "https://github.com/autoframe",
    "require": {
        "php": "^7.4 || ^8.0",
        "autoframe/components-exception": "^1.0 || ^2.0",
        "autoframe/components-filemime": "^3.0",
        "autoframe/design-patterns": "^1.0"
    },
    "version": "2.0.2",
    "authors": [
        {
            "name": "Nistor Alexandru Marius",
            "homepage": "https://autoframe.ro",
            "role": "Developer"
        }
    ],
    "keywords": ["File System", "Looping", "Traversing", "Versioning", "Encode", "Base64", "Mime" ],
    "autoload": {
        "psr-4": {
            "Autoframe\\Components\\FileSystem\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Unit\\": "Tests/Unit"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6 || ^10"
    },
    "optimize-autoloader": true

}
